{
  "name": "lu9wt-qsl-creator",
  "version": "1.0.0-beta",
  "displayName": "Digital QSL Card Creator",
  "description": "Herramienta para la creación y envío de tarjetas QSL para radioaficionados. Diseñada para ser intuitiva y eficiente, permitiendo generar tarjetas QSL personalizadas con información del QSO.",
  "main": "./out/main/index.js",
  "author": "JSDRAKE - LU9WT",
  "homepage": "https://www.jsdrake.com.ar",
  "license": "MIT",
  "email": "jsdrake@jsdrake.com.ar",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "dev:clean": "node -e \"console.clear()\" && electron-vite dev",
    "build": "electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@react-oauth/google": "^0.12.2",
    "axios": "^1.9.0",
    "dotenv": "^17.0.1",
    "electron-store": "^10.1.0",
    "electron-updater": "^6.3.9",
    "fast-xml-parser": "^5.2.5",
    "html-to-image": "^1.11.13",
    "nodemailer": "^7.0.3",
    "prop-types": "^15.8.1",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^2.0.0",
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "electron": "^35.1.5",
    "electron-builder": "^25.1.8",
    "electron-vite": "^3.1.0",
    "eslint": "^9.24.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "vite": "^6.2.6"
  }
}
